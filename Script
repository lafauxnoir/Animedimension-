-- Interface Roblox custom
local gui = Instance.new("ScreenGui", game.CoreGui)
local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 250, 0, 300)
frame.Position = UDim2.new(0.5, -125, 0.5, -150)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BorderSizePixel = 0

local function createButton(text, positionY, callback)
    local button = Instance.new("TextButton", frame)
    button.Size = UDim2.new(0, 230, 0, 40)
    button.Position = UDim2.new(0, 10, 0, positionY)
    button.Text = text
    button.TextColor3 = Color3.new(1, 1, 1)
    button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    button.BorderSizePixel = 0
    button.MouseButton1Click:Connect(callback)
end

-- Fonctions (à adapter selon ton jeu)
createButton("Auto Farm", 10, function()
    print("Auto Farm lancé !")
    -- Mets ici ton code de farm
end)

createButton("TP Zone", 60, function()
    print("Téléportation...")
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 10, 0)
end)

createButton("God Mode", 110, function()
    print("God Mode activé")
    game.Players.LocalPlayer.Character.Humanoid.MaxHealth = math.huge
    game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
end)

createButton("Attack Speed", 160, function()
    print("Vitesse d’attaque augmentée")
    for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v:IsA("Tool") then
            v.Activated:Connect(function()
                v:Activate()
                wait(0.05)
            end)
        end
    end
end)

createButton("Fermer", 210, function()
    gui:Destroy()
end)
